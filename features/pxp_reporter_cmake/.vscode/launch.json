    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceRoot}/project",
            "executable": "./build/pxp_reporter.elf",
            "name": "Debug Microcontroller",
            "gdbPath": "gdb-multiarch",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "jlink",
            "device": "DA14695",
            "interface": "swd",
            "rtos": "FreeRTOS",
            "rttConfig": {
                "enabled": true,
                "address": "auto",
                "decoders": [
                    {
                        "label": "",
                        "port": 0,
                        "type": "console"
                    }
                ]
            },
        }
        ]
